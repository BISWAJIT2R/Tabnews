{
    "manifest_version": 3,
    "name": "TabNews",
    "description": "Just click and get all the top-news",
    "version": "0.0.1",
    "icons": {
        "48": "icons/icons8-fox-48.png",
        "128": "icons/icons8-fox-96.png"
    },
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab",
        "https://newsapi.org/"
    ]
}